{
  "name": "open-world-adventure",
  "version": "1.0.0",
  "description": "A real time open world multiplayer game.",
  "main": "index.js",
  "scripts": {
    "start": "nodemon Server/index.js",
    "build": "npx tailwindcss -i ./Assets/Styles/input.css -o ./Assets/Styles/style.css --watch",

    "uglifyLibraries": "uglifyjs ./Assets/phaser/dist/phaser.js ./Assets/webfontloader/webfontloader.js ./Assets/crypto-js/crypto-js.js -o ./Assets/MinLibrary/libraries.min.js -c -m",

    "uglify404": "uglifyjs ./404pages/404phaser.js -o ./404pages/MinJS/404phaser.min.js -c -m",

    "uglifyCookieJS": "uglifyjs ./Public/clearCookie.js -o ./Public/MinJS/clearCookie.min.js -c -m",
    "uglifyLobbyJS": "uglifyjs ./Public/lobby.js -o ./Public/MinJS/lobby.min.js -c -m",
    "uglifyLobbyPhaserJS": "uglifyjs ./Public/scenePreloadPhaser.js ./Public/lobbyPhaser.js ./Public/lobbyPhaserSocket.js ./Public/lobbyConfig.js -o ./Public/MinJS/lobbyPhaserUglify.min.js -c -m"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JohnRendell/Open-World-Adventure.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JohnRendell/Open-World-Adventure/issues"
  },
  "homepage": "https://github.com/JohnRendell/Open-World-Adventure#readme",
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.7",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "form-data": "^4.0.1",
    "mongoose": "^8.6.3",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.7.0",
    "nodemon": "^3.1.7",
    "phaser": "^3.87.0",
    "sanitize-html": "^2.14.0",
    "socket.io": "^4.8.1",
    "uglify-js": "^3.19.3",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.16"
  }
